<html xmlns="http://www.w3.org/1999/xhtml">
<head>
  <meta content="text/html; charset=iso-8859-1" http-equiv="Content-Type" />
  <title>Meta Admin - Administration Clever Panel</title>
  <%= stylesheet_link_tag    "admin/admin", :media => "all" %>
  <%= javascript_include_tag "admin/admin", "jquery.tokeninput" %>
  <%= tinymce_assets %>
  <%= csrf_meta_tags %>
  <%#*<link href="css/meta-admin.css" type="text/css" rel="stylesheet"></link>%>
  
  <%= yield :head %>
  <!-- blue theme is default -->

  <!--[if lte IE 6]><link media="screen" rel="stylesheet" type="text/css" href="css/ie.css" /><![endif]-->
  <%#*<script src="js/css.js" type="text/javascript"></script>%>
  <%#*<link media="screen" href="css/js.css" type="text/css" rel="stylesheet"></link>%>
  <%#*<script src="js/behaviour.js" type="text/javascript"></script>%>
</head>

<body>
  <div id="wrapper">
    <div id="head">
      <div class="inner">
        <%= render :partial => "admin/shared/head" %>
      </div>
    </div>
    <div id="content">
      <div id="content_bottom">
        <div class="inner">
          <div id="info">
            <%= yield %>
          </div>
          <div id="sidebar">
            <%= yield :sidebar %>
          </div>
        </div>
      </div>
    </div>
  </div>
  <%= render :partial => "admin/shared/footer" %>
</body>
</html>